{
  "manifest_version": 2,

  "name": "Mute Tabs",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
